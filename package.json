{
  "name": "hedronnc-api",
  "version": "1.0.0",
  "description": "Hedron NewCo Backend API",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t hedronnc-api .",
    "docker:run": "docker run -p 3000:3000 -d hedronnc-api",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "dev": "npm run build && node -r source-map-support/register .",
    "start": "node .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.3",
    "@loopback/core": "^2.8.0",
    "@loopback/openapi-v3": "^3.4.3",
    "@loopback/repository": "^2.7.0",
    "@loopback/rest": "^5.1.1",
    "@loopback/rest-explorer": "^2.2.4",
    "@loopback/service-proxy": "^2.3.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.3",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.1.7",
    "@types/node": "^10.17.26",
    "@loopback/eslint-config": "^8.0.1",
    "eslint": "^7.2.0",
    "typescript": "~3.9.5"
  }
}
